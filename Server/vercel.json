{
    "version": 2,
    "name": "Server",
    "builds": [
       { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/" }
    ],
    
"rewrites": [
          {
            "source": "/api/(.*)",
            "destination": "https://job-portal-client-puce.vercel.app"
          }
        ]
      
 }